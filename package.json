{
  "name": "danadajian.github.io",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "husky": "8.0.1",
    "typescript": "4.7.4",
    "react-refresh": "0.10.0"
  },
  "scripts": {
    "build": "react-scripts build && cp -r build/. .",
    "lint": "eslint --fix 'src/**'",
